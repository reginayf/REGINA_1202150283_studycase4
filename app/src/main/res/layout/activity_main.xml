<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.regina.studycasemodul4regina.MainActivity">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_centerInParent="true"
        android:label="Modul 4">

        <Button
            android:onClick="yes"
            android:id="@+id/btn_carigambar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Pencarian Gambar"
            tools:layout_editor_absoluteX="117dp"
            tools:layout_editor_absoluteY="121dp" />
        <Button
            android:onClick="yes"
        android:id="@+id/btn_listmahasiswa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="List Nama Mahasiswa"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="121dp" />
    </LinearLayout>


</RelativeLayout>
